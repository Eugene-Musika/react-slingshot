{
	"name": "react-slingshot-muse",
	"version": "1.0.0",
	"description": "Starter kit for creating apps with React and Redux",
	"engines": {
		"npm": ">=3"
	},
	"scripts": {
		"preinstall": "node tools/nodeVersionCheck.js",
		"setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
		"remove-demo": "babel-node tools/removeDemo.js",
		"start-message": "babel-node tools/startMessage.js",
		"prestart": "npm run start-message",
		"start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
		"open:src": "babel-node tools/srcServer.js",
		"open:dist": "babel-node tools/distServer.js",
		"lint": "esw webpack.config.* src tools --color",
		"lint:watch": "npm run lint -- --watch",
		"remove-dist": "rimraf ./dist",
		"prebuild": "npm run remove-dist && npm run lint && npm run test",
		"build": "babel-node tools/build.js && npm run open:dist",
		"build:stage": "npm run remove-dist && babel-node tools/buildStage.js",
		"test": "jest",
		"test:CI": "babel-node tools/testCi.js",
		"test:cover": "npm run test -- --coverage ",
		"test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
		"test:watch": "jest --watch",
		"open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
		"analyze:bundle": "babel-node ./tools/analyzeBundle.js",
		"analyze:src": "babel-node ./tools/analyzeSrc.js && opn ./analyze/index.html"
	},
	"author": "Eugene Musika",
	"license": "MIT",
	"dependencies": {
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.6",
		"react-router-dom": "^4.2.2",
		"react-router-redux": "5.0.0-alpha.6",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.0.3",
		"babel-jest": "^21.2.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-lodash": "^3.3.2",
		"babel-plugin-react-css-modules": "^3.3.2",
		"babel-plugin-transform-react-constant-elements": "^6.23.0",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.10",
		"babel-plugin-transform-remove-console": "^6.8.5",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-stage-0": "^6.24.1",
		"browser-sync": "^2.18.13",
		"chalk": "^2.3.0",
		"concurrently": "^3.5.1",
		"connect-history-api-fallback": "^1.5.0",
		"coveralls": "^3.0.0",
		"css-loader": "^0.28.7",
		"css-mqpacker": "^6.0.1",
		"cssnano": "^3.10.0",
		"enzyme": "^3.2.0",
		"enzyme-adapter-react-16": "^1.1.0",
		"eslint": "^4.12.1",
		"eslint-plugin-flowtype": "^2.39.1",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-react": "^7.5.1",
		"eslint-watch": "^3.1.3",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.5",
		"history": "^4.7.2",
		"html-webpack-plugin": "^2.30.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^21.2.1",
		"jest-cli": "^21.2.1",
		"json-loader": "^0.5.7",
		"lodash-webpack-plugin": "^0.11.4",
		"mockdate": "^2.0.2",
		"node-sass": "^4.7.2",
		"opn-cli": "^3.1.0",
		"plato": "^1.7.0",
		"postcss-browser-reporter": "^0.5.0",
		"postcss-center": "^1.1.0",
		"postcss-circle": "^1.1.0",
		"postcss-cssnext": "^3.0.2",
		"postcss-hamster": "^1.3.0",
		"postcss-loader": "^2.0.9",
		"postcss-merge-rules": "^2.1.2",
		"postcss-nested": "^2.1.2",
		"postcss-pxtorem": "^4.0.1",
		"postcss-scss": "^1.0.2",
		"postcss-short": "^4.1.0",
		"postcss-svgo": "^2.1.6",
		"postcss-triangle": "^2.0.0",
		"postcss-utilities": "^0.7.0",
		"postcss-vmin": "^3.0.0",
		"postcss-will-change": "^2.0.0",
		"prompt": "^1.0.0",
		"prop-types": "^15.6.0",
		"raf": "^3.4.0",
		"react-hot-loader": "^3.1.3",
		"react-test-renderer": "^16.2.0",
		"redux-immutable-state-invariant": "^2.1.0",
		"redux-promise-middleware": "^5.0.0",
		"replace": "^0.3.0",
		"rimraf": "^2.6.2",
		"rucksack-css": "^1.0.2",
		"sass-loader": "^6.0.6",
		"script-ext-html-webpack-plugin": "^1.8.8",
		"semantic-ui-react": "0.77.0",
		"semantic-ui-sass": "^2.2.11",
		"style-loader": "^0.19.0",
		"stylelint": "^8.3.1",
		"stylelint-config-sass-guidelines": "^4.0.1",
		"stylelint-order": "^0.7.0",
		"stylelint-processor-html": "^1.0.0",
		"stylelint-scss": "^2.2.0",
		"url-loader": "^0.6.2",
		"webpack": "^3.9.1",
		"webpack-build-notifier": "^0.1.18",
		"webpack-bundle-analyzer": "^2.9.1",
		"webpack-dev-middleware": "^1.12.2",
		"webpack-hot-middleware": "^2.21.0",
		"webpack-md5-hash": "^0.0.6",
		"webpack-pwa-manifest": "3.3.2"
	},
	"keywords": [
		"react",
		"reactjs",
		"react-router",
		"hot",
		"reload",
		"hmr",
		"live",
		"edit",
		"webpack",
		"redux",
		"flux",
		"boilerplate",
		"starter"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Eugene-Musika/react-slingshot"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
			"\\.(css|less|scss)$": "<rootDir>/tools/assetsTransformer.js"
		},
		"setupFiles": [
			"raf/polyfill",
			"./tools/enzymeTestAdapterSetup.js"
		]
	},
	"babel": {
		"plugins": [
			"transform-class-properties"
		],
		"env": {
			"development": {
				"presets": [
					"env",
					"react",
					"react-hmre",
					"stage-0"
				],
				"plugins": [
					[
						"react-css-modules",
						{
							"generateScopedName": "[local]__[hash:base64]",
							"filetypes": {
								".scss": {
									"syntax": "postcss-scss",
									"plugins": [
										"postcss-nested"
									]
								}
							},
							"webpackHotModuleReloading": true
						}
					]
				]
			},
			"production": {
				"presets": [
					[
						"env",
						{
							"es2015": {
								"modules": false
							},
							"targets": {
								"ie": 10,
								"ios": 7,
								"browsers": [
									"last 2 versions",
									">5%"
								],
								"uglify": true
							}
						}
					],
					"react",
					"stage-0"
				],
				"plugins": [
					"transform-react-constant-elements",
					"transform-react-remove-prop-types",
					[
						"transform-remove-console",
						{
							"exclude": [
								"info"
							]
						}
					],
					[
						"react-css-modules",
						{
							"generateScopedName": "[local]__[hash:base64]",
							"filetypes": {
								".scss": {
									"syntax": "postcss-scss",
									"plugins": [
										"postcss-nested"
									]
								}
							},
							"webpackHotModuleReloading": false
						}
					]
				]
			},
			"stage": {
				"presets": [
					[
						"env",
						{
							"es2015": {
								"modules": false
							},
							"targets": {
								"ie": 10,
								"ios": 7,
								"browsers": [
									"last 2 versions",
									">5%"
								]
							}
						}
					],
					"react",
					"stage-0"
				],
				"plugins": [
					[
						"react-css-modules",
						{
							"generateScopedName": "[local]__[hash:base64]",
							"filetypes": {
								".scss": {
									"syntax": "postcss-scss",
									"plugins": [
										"postcss-nested"
									]
								}
							},
							"webpackHotModuleReloading": false
						}
					]
				]
			},
			"test": {
				"presets": [
					"env",
					"react",
					"stage-0"
				]
			}
		}
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"commonjs": true,
			"node": true,
			"es6": true,
			"worker": true,
			"jquery": true,
			"mocha": true,
			"jest": true
		},
		"globals": {
			"__DEV__": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:import/errors",
			"plugin:import/warnings",
			"plugin:react/recommended"
		],
		"parser": "babel-eslint",
		"parserOptions": {
			"sourceType": "module",
			"ecmaVersion": 8,
			"ecmaFeatures": {
				"experimentalObjectRestSpread": true,
				"impliedStrict": true,
				"jsx": true
			}
		},
		"plugins": [
			"react",
			"flowtype"
		],
		"rules": {
			"accessor-pairs": 2,
			"array-bracket-spacing": [
				1,
				"never"
			],
			"array-callback-return": 1,
			"arrow-body-style": [
				1,
				"as-needed"
			],
			"arrow-parens": [
				1,
				"as-needed"
			],
			"block-scoped-var": 1,
			"block-spacing": [
				1,
				"always"
			],
			"brace-style": [
				1,
				"1tbs",
				{
					"allowSingleLine": true
				}
			],
			"camelcase": 1,
			"comma-dangle": 1,
			"comma-spacing": 1,
			"comma-style": 1,
			"complexity": 1,
			"computed-property-spacing": 1,
			"curly": 1,
			"dot-location": [
				1,
				"property"
			],
			"dot-notation": 1,
			"eol-last": 1,
			"for-direction": 2,
			"func-call-spacing": 1,
			"func-name-matching": 1,
			"func-style": [
				1,
				"declaration",
				{
					"allowArrowFunctions": true
				}
			],
			"generator-star-spacing": [
				1,
				{
					"before": false,
					"after": true
				}
			],
			"key-spacing": 1,
			"keyword-spacing": 1,
			"max-depth": 1,
			"max-len": [
				1,
				{
					"code": 120,
					"tabWidth": 2,
					"ignoreTrailingComments": true,
					"ignoreUrls": true,
					"ignoreStrings": true,
					"ignoreTemplateLiterals": true,
					"ignoreRegExpLiterals": true
				}
			],
			"max-params": [
				1,
				{
					"max": 5
				}
			],
			"max-statements-per-line": [
				1,
				{
					"max": 2
				}
			],
			"multiline-ternary": [
				1,
				"always-multiline"
			],
			"new-cap": 2,
			"no-array-constructor": 1,
			"no-await-in-loop": 1,
			"no-bitwise": [
				1,
				{
					"allow": [
						"~",
						"^"
					]
				}
			],
			"no-caller": 2,
			"no-confusing-arrow": [
				1,
				{
					"allowParens": true
				}
			],
			"no-console": 0,
			"no-duplicate-imports": 1,
			"no-else-return": 1,
			"no-empty": 1,
			"no-empty-function": 1,
			"no-eq-null": 2,
			"no-eval": 2,
			"no-extra-bind": 1,
			"no-extra-label": 2,
			"no-extra-parens": [
				1,
				"all",
				{
					"ignoreJSX": "all"
				}
			],
			"no-floating-decimal": 1,
			"no-implied-eval": 2,
			"no-lone-blocks": 1,
			"no-lonely-if": 1,
			"no-loop-func": 1,
			"no-multi-str": 1,
			"no-multiple-empty-lines": [
				1,
				{
					"max": 5
				}
			],
			"no-negated-condition": 1,
			"no-nested-ternary": 1,
			"no-new-object": 1,
			"no-return-assign": 1,
			"no-return-await": 1,
			"no-script-url": 2,
			"no-self-compare": 2,
			"no-template-curly-in-string": 2,
			"no-unmodified-loop-condition": 1,
			"no-unneeded-ternary": 1,
			"no-unused-vars": [1, {
				"argsIgnorePattern": "^_"
			}],
			"no-use-before-define": [
				2,
				{
					"functions": false
				}
			],
			"no-useless-computed-key": 1,
			"no-useless-concat": 1,
			"no-useless-constructor": 1,
			"no-useless-rename": 1,
			"no-useless-return": 1,
			"no-var": 1,
			"no-void": 2,
			"no-whitespace-before-property": 1,
			"no-with": 2,
			"object-curly-newline": [
				1,
				{
					"multiline": true
				}
			],
			"object-curly-spacing": [
				1,
				"always"
			],
			"object-shorthand": 1,
			"one-var-declaration-per-line": 1,
			"operator-assignment": 1,
			"prefer-arrow-callback": 1,
			"prefer-const": [
				1,
				{
					"ignoreReadBeforeAssign": true
				}
			],
			"prefer-destructuring": [
				1,
				{
					"array": false,
					"object": true
				}
			],
			"prefer-rest-params": 1,
			"prefer-spread": 1,
			"quote-props": [
				1,
				"as-needed"
			],
			"quotes": [
				"warn",
				"single"
			],
			"radix": [
				1,
				"as-needed"
			],
			"rest-spread-spacing": 1,
			"semi": [
				"warn",
				"always"
			],
			"semi-spacing": 1,
			"semi-style": 1,
			"sort-imports": 1,
			"sort-vars": 1,
			"sort-keys": 1,
			"space-before-function-paren": [
				1,
				{
					"anonymous": "never",
					"named": "always",
					"asyncArrow": "ignore"
				}
			],
			"space-in-parens": 1,
			"space-unary-ops": [
				1,
				{
					"words": true,
					"nonwords": false
				}
			],
			"spaced-comment": [
				1,
				"always",
				{
					"markers": [
						"!",
						"*",
						"?",
						"//",
						"TODO:",
						"FIXME:",
						"FUTURE:",
						"NOTE:",
						"CHANGES:"
					],
					"exceptions": [
						"-",
						"=",
						"#",
						"*"
					]
				}
			],
			"template-curly-spacing": [
				1,
				"always"
			],
			"valid-jsdoc": [
				1,
				{
					"requireParamDescription": false,
					"requireReturnDescription": false,
					"requireReturn": false
				}
			],
			"vars-on-top": 1,
			"yield-star-spacing": [
				1,
				"after"
			],
			"indent": "off",
			"indent-legacy": [
				"warn",
				"tab",
				{
					"SwitchCase": 1,
					"MemberExpression": 1,
					"FunctionDeclaration": {
						"parameters": 0
					},
					"FunctionExpression": {
						"parameters": 0
					},
					"CallExpression": {
						"parameters": 0
					},
					"VariableDeclarator": {
						"var": 2,
						"let": 2,
						"const": 3
					}
				}
			],
			"linebreak-style": [
				"off",
				"unix"
			],
			"no-global-assign": 2,
			"no-undef-init": 1,
			"newline-after-var": 1,
			"require-await": 2,
			"react/jsx-curly-spacing": [
				1,
				{
					"when": "always",
					"children": true
				}
			],
			"react/jsx-pascal-case": 1,
			"react/jsx-wrap-multilines": 2,
			"react/no-danger": 1,
			"react/no-did-mount-set-state": 1,
			"react/no-did-update-set-state": 1,
			"react/no-multi-comp": 1,
			"react/no-set-state": 0,
			"react/prefer-es6-class": 1,
			"import/extensions": [
				1,
				{
					"css": "always",
					"scss": "always"
				}
			],
			"react/self-closing-comp": 1,
			"react/sort-comp": 1,
			"react/display-name": [
				1,
				{
					"ignoreTranspilerName": false
				}
			],
			"react/forbid-prop-types": [
				1,
				{
					"forbid": [
						"any"
					]
				}
			]
		}
	},
	"stylelint": {
		"extends": "stylelint-config-sass-guidelines",
		"processors": [
			"stylelint-processor-html"
		],
		"plugins": [
			"stylelint-order"
		],
		"rules": {
			"indentation": [
				"tab",
				{
					"severity": "warning"
				}
			],
			"max-empty-lines": [
				2,
				{
					"severity": "warning"
				}
			],
			"declaration-empty-line-before": [
				"always",
				{
					"except": [
						"after-declaration",
						"first-nested"
					],
					"ignore": [
						"after-comment",
						"inside-single-line-block"
					],
					"severity": "warning"
				}
			],
			"no-eol-whitespace": null,
			"at-rule-empty-line-before": [
				"always",
				{
					"except": [
						"first-nested",
						"blockless-after-same-name-blockless"
					],
					"ignore": [
						"after-comment"
					],
					"severity": "warning"
				}
			],
			"number-max-precision": [
				4,
				{
					"severity": "warning"
				}
			],
			"number-no-trailing-zeros": [
				true,
				{
					"severity": "warning"
				}
			],
			"string-quotes": [
				"double",
				{
					"severity": "warning"
				}
			],
			"block-closing-brace-empty-line-before": [
				"never",
				{
					"severity": "warning"
				}
			],
			"length-zero-no-unit": [
				true,
				{
					"severity": "warning"
				}
			],
			"time-min-milliseconds": [
				100,
				{
					"severity": "warning"
				}
			],
			"color-hex-length": [
				"short",
				{
					"severity": "warning"
				}
			],
			"shorthand-property-no-redundant-values": [
				true,
				{
					"severity": "warning"
				}
			],
			"selector-attribute-quotes": "always",
			"selector-no-qualifying-type": null,
			"selector-max-id": 0,
			"no-descending-specificity": [
				true,
				{
					"severity": "warning"
				}
			],
			"max-nesting-depth": [
				5,
				{
					"severity": "warning",
					"ignore": [
						"blockless-at-rules"
					]
				}
			],
			"selector-list-comma-newline-after": [
				"always",
				{
					"severity": "warning"
				}
			],
			"no-missing-end-of-source-newline": null,
			"order/properties-alphabetical-order": null,
			"selector-max-compound-selectors": 5,
			"selector-class-pattern": [
				"^([a-z0-9]+(-{1,2}|_{2})?)*$",
				{
					"resolveNestedSelectors": true
				}
			],
			"order/order": [
				[
					"custom-properties",
					"dollar-variables",
					{
						"type": "at-rule",
						"name": "extend"
					},
					"declarations",
